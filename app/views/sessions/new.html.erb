<h1>Log In</h1>   
<%= form_tag sessions_path do %>   
<div>   
<%= label_tag :email %><br>   
<%= text_field_tag :email %>   
</div>   
  <div>   
    <%= label_tag :password %><br>   
<%= password_field_tag :password %>   
</div>   
  <div>   
    <%= submit_tag "Log In" %>
   
</div>   
<h3><%= link_to "USERS LIST", users_path %></h3>
<% end %>


  	